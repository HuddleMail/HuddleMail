<%- if controller_name != 'sessions' %>
  <%= button_to "Log in", new_session_path(resource_name), class: 'button',:method => :get %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= button_to "Sign up", new_registration_path(resource_name), class: 'button',:method => :get %><br />
<% end -%>
